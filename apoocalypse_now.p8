--explosions

explosions={}

function draw_explosions()
 for xplo in all(explosions) do
  xplo:draw()
 end
end

function explosion(sx,sy)
 return {
 x=sx,
 y=sy,
 age=0,
 kill=function(self)
  for hoarder in all(hoarders) do
  if (hoarder.x>self.x-15 and hoarder.x<self.x+15) then
  if (hoarder.y>self.y-15 and hoarder.y<self.y+15) then
  hoarder:kill()
  end end
  end
 end,
 draw=function(self)
  if(self.age==0) then
  self:kill()
  end
  local size=20-self.age
  local colour=9
  circfill(self.x,self.y,size+1,0)
  circfill(self.x,self.y,size,colour)
  self.age=self.age+1
  if self.age>4 then explosions[self]=nil end
 end
 }
end
